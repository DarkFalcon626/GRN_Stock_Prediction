{
	"Data":{
		"tickers": ["AAPL", "MSFT", "GOOGL", "AMZN", "META", "ADBE", "CRM", "CSCO", "ORCL"],
		"features": ["Open", "High", "Low", "Close", "Volume"],
		"Start_date": "2010-01-01",
		"Seq_length": 60,
		"forecast_steps": 7,
		"batch_size": 16,
		"split_ratio": 0.80
	},
	
	"Model":{
		"encoder_input_dim": 5,
		"decoder_input_dim": 1,
		"gcn_hidden_dim": 12,
		"encoder_gru_hidden_dim": 24,
		"decoder_gru_hidden_dim": 24
	},
	
	"Train":{		
		"num_epochs": 15,
		"teacher_forcing_ratio": 0.5,
		"lr": 0.001
	}
}